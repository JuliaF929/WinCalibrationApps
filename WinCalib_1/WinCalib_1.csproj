<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
	
	<GenerateAssemblyInfo>true</GenerateAssemblyInfo>
	<ContinuousIntegrationBuild>false</ContinuousIntegrationBuild>
	<IncludeSourceRevisionInInformationalVersion>false</IncludeSourceRevisionInInformationalVersion>
	
	  <!-- Assembly info -->
	<Company>Calibrix</Company>
	<Product>Win Calib Demo</Product>
	<AssemblyTitle>Win Calib Demo</AssemblyTitle>
	<Description>Win Calib Demo (to be integrated in Calibrix)</Description>

	<!-- Version info -->
	<Version>1.0.0.1</Version>
	<FileVersion>1.0.0.1</FileVersion>
	<AssemblyVersion>1.0.0.1</AssemblyVersion>
	<InformationalVersion>1.0.0.1</InformationalVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

</Project>
